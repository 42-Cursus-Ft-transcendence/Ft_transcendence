{
  "name": "fastify-app",
  "version": "1.0.0",
  "scripts": {
    "dev": "concurrently -k --prefix none \"npm:watch:ts\" \"npm:watch:dev\"",
    "watch:ts": "tsc --noEmit --watch",
    "watch:dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js"
  },
  "dependencies": {
    "@ethersproject/experimental": "^5.8.0",
    "@fastify/cookie": "^11.0.2",
    "@fastify/jwt": "^9.1.0",
    "@fastify/static": "^8.2.0",
    "@fastify/websocket": "^11.1.0",
    "@fastify/oauth2": "^8.1.0",
    "fastify-plugin": "^5.0.1",
    "bcrypt": "^6.0.0",
    "dotenv": "^16.5.0",
    "ether.js": "^1.0.0",
    "ethers": "^6.14.4",
    "fastify": "^5.4.0",
    "sqlite3": "^5.1.6",
    "@types/qrcode": "^1.5.5",
    "qrcode": "^1.5.4",
    "otplib": "^12.0.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/node": "^24.0.3",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.0.0",
    "pino-pretty": "^13.0.0"
  }
}
